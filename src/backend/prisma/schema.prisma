
// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Licitation {
  id        Int      @id @default(autoincrement())
  title     String
  cnpj      String
  object    String
  deadline  DateTime
  createdAt DateTime @default(now())
  
  @@map("licitations")
}
